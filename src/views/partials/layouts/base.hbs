<!doctype html>
<html>
  <head>
    <title>
      {{#if title}}
        {{title}}
      {{else}}
        WEBSITE
      {{/if}}
    </title>
    <link rel="stylesheet" href="assets/css/styles.css">
    {{!-- <link rel="stylesheet" href="assets/css/styles.min.css"> --}}
    {{#> head-block}}
    {{!-- stylesheetsLibs content goes here. --}}
    {{> includes/stylesheetsLibs }}
    {{/head-block}}
  </head>
  <body>
{{!-- 
// ------------- HEADER
--}}   
    {{#> header-block}}
      {{!-- Header content goes here. --}}
      {{> components/header/navbar }}
    {{/header-block}}

    {{#> main-block}}
      {{!--Main content goes here. --}}
      {{> components/main/main }}
    {{/main-block}}
{{!-- 
// ------------- FOOTER 
--}}
    <footer>
      {{#> footer-block}}
        {{!-- Footer content goes here. --}}
        {{> components/footer/footer }}
      {{/footer-block}}
    </footer>
{{!-- 
// ------------- SCRIPTS
--}}
    <script src="assets/js/functions.min.js"></script>
    {{#> scripts-block}}
    {{!-- scriptsLibs content goes here. --}}
    {{> includes/scriptsLibs }}
    {{/scripts-block}}
  </body>
</html>
